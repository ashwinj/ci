void swap(int* x, int* y) {
	int temp;
	temp = *x;
	*x = *y;
	*y = temp;
}

void sort(int* arr, int sz) {
	int x, y;
	for(x=0; x < sz; x = x+1) {
		for(y=0; y < sz-1; y=y+1) {
			if(arr[y] > arr[y+1]) {
				swap(&arr[y], &arr[y+1]);
			}
		}
	}
}

int main() {
	int i;
	int x = 15, y = 20;
	int z[10] = {1,4,5,2,6,7,3,0,9,8};
	printf("Before sorting:\n");
	for(i = 0; i < 10; i = i+1) printf("%d\t",z[i]);
	printf("\n");
	sort(z, 10);
	printf("After sorting:\n");
	for(i = 0; i < 10; i = i+1) printf("%d\t",z[i]);
	printf("\n");
	return 0;
}
